CREATE TABLE BOOKING (
  ID_BOOKING     int IDENTITY NOT NULL, 
  NAME_BOOKING   nvarchar(255) NULL, 
  AMOUNT_BOOKING int NULL, 
  DETAIL_BOOKING nvarchar(255) NULL, 
  STATUS_BOOKING nvarchar(255) NULL, 
  ID_POD         int NOT NULL, 
  ID_CUSTOMER    int NOT NULL, 
  ID_PAYMENT     int NOT NULL, 
  PRIMARY KEY (ID_BOOKING));


CREATE TABLE [BOOKING PRODUCT] (
  ID_BOOKINGPRODUCT     int IDENTITY NOT NULL, 
  NAME_BOOKINGPRODUCT   nvarchar(255) NULL, 
  AMOUNT_BOOKINGPRODUCT int NULL, 
  DETAIL_BOOKINGPRODUCT nvarchar(255) NULL, 
  ID_PRODUCT            int NOT NULL, 
  ID_BOOKING            int NOT NULL, 
  PRIMARY KEY (ID_BOOKINGPRODUCT));


CREATE TABLE CUSTOMER (
  ID_CUSTOMER       int IDENTITY NOT NULL, 
  NAME_CUSTOMER     nvarchar(255) NULL, 
  EMAIL_CUSTOMER    nvarchar(255) NULL, 
  PHONENB_CUSTOMER  int NULL, 
  BIRTHDAY_CUSTOMER date NULL, 
  TYPE_CUSTOMER     nvarchar(255) NULL, 
  POINT_CUSTOMER    int NULL, 
  PRIMARY KEY (ID_CUSTOMER));


CREATE TABLE FEEDBACK (
  ID_FEEDBACK     int IDENTITY NOT NULL, 
  CMT_FEEDBACK    nvarchar(255) NULL, 
  RATING_FEEDBACK int NULL, 
  DATE_FEEDBACK   date NULL, 
  ID_BOOKING      int NOT NULL, 
  ID_CUSTOMER     int NOT NULL, 
  PRIMARY KEY (ID_FEEDBACK));


CREATE TABLE PAYMENT (
  ID_PAYMENT     int IDENTITY NOT NULL, 
  METHOD_PAYMENT int NULL, 
  NAME_PAYMENT   nvarchar(255) NULL, 
  PRIMARY KEY (ID_PAYMENT));


CREATE TABLE POD (
  ID_POD        int IDENTITY NOT NULL, 
  NAME_POD      nvarchar(255) NULL, 
  STATUS_POD    nvarchar(255) NULL, 
  PRICE_POD     int NULL, 
  RATING_POD    int NULL, 
  ID_POD_TYPE   int NOT NULL, 
  ID_STORE      int NOT NULL, 
  ID_PRICEFRAME int NOT NULL, 
  PRIMARY KEY (ID_POD));


CREATE TABLE [POD PRICE FRAME] (
  ID_PRICEFRAME     int IDENTITY NOT NULL, 
  NAME_PRICEFRAME   nvarchar(255) NULL, 
  DETAIL_PRICEFRAME nvarchar(255) NULL, 
  PRIMARY KEY (ID_PRICEFRAME));


CREATE TABLE [POD Type] (
  ID_POD_TYPE   int IDENTITY NOT NULL, 
  NAME_POD_TYPE nvarchar(255) NULL, 
  PRIMARY KEY (ID_POD_TYPE));


CREATE TABLE PRODUCT (
  ID_PRODUCT     int IDENTITY NOT NULL, 
  NAME_PRODUCT   nvarchar(255) NULL, 
  STATUS_PRODUCT nvarchar(255) NULL, 
  RATING_PRODUCT int NULL, 
  PRICE_PRODUCT  int NULL, 
  ID_STORE       int NOT NULL, 
  PRIMARY KEY (ID_PRODUCT));


CREATE TABLE SLOT (
  ID_SLOT   int IDENTITY NOT NULL, 
  NAME_SLOT nvarchar(255) NULL, 
  ID_POD    int NOT NULL, 
  PRIMARY KEY (ID_SLOT));


CREATE TABLE SLOT_BOOKING (
  ID_SLOT    int NOT NULL, 
  ID_BOOKING int NOT NULL, 
  PRIMARY KEY (ID_SLOT, 
  ID_BOOKING));


CREATE TABLE STORE (
  ID_STORE   int IDENTITY NOT NULL, 
  NAME_STORE nvarchar(255) NULL, 
  PRIMARY KEY (ID_STORE));


CREATE TABLE Utility (
  ID_UTILITY     int IDENTITY NOT NULL, 
  NAME_UTILITY   nvarchar(255) NULL, 
  DETAIL_UTILITY nvarchar(255) NULL, 
  PRIMARY KEY (ID_UTILITY));


CREATE TABLE Utility_POD (
  ID_UTILITY int NOT NULL, 
  ID_POD     int NOT NULL, 
  PRIMARY KEY (ID_UTILITY, 
  ID_POD));


ALTER TABLE POD ADD CONSTRAINT FKPOD948483 FOREIGN KEY (ID_POD_TYPE) REFERENCES [POD Type] (ID_POD_TYPE);
ALTER TABLE POD ADD CONSTRAINT FKPOD963503 FOREIGN KEY (ID_STORE) REFERENCES STORE (ID_STORE);
ALTER TABLE PRODUCT ADD CONSTRAINT FKPRODUCT533869 FOREIGN KEY (ID_STORE) REFERENCES STORE (ID_STORE);
ALTER TABLE [BOOKING PRODUCT] ADD CONSTRAINT [FKBOOKING PR153301] FOREIGN KEY (ID_PRODUCT) REFERENCES PRODUCT (ID_PRODUCT);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING55743 FOREIGN KEY (ID_POD) REFERENCES POD (ID_POD);
ALTER TABLE [BOOKING PRODUCT] ADD CONSTRAINT [FKBOOKING PR336465] FOREIGN KEY (ID_BOOKING) REFERENCES BOOKING (ID_BOOKING);
ALTER TABLE POD ADD CONSTRAINT FKPOD609662 FOREIGN KEY (ID_PRICEFRAME) REFERENCES [POD PRICE FRAME] (ID_PRICEFRAME);
ALTER TABLE SLOT ADD CONSTRAINT FKSLOT934327 FOREIGN KEY (ID_POD) REFERENCES POD (ID_POD);
ALTER TABLE FEEDBACK ADD CONSTRAINT FKFEEDBACK225801 FOREIGN KEY (ID_BOOKING) REFERENCES BOOKING (ID_BOOKING);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING783431 FOREIGN KEY (ID_CUSTOMER) REFERENCES CUSTOMER (ID_CUSTOMER);
ALTER TABLE FEEDBACK ADD CONSTRAINT FKFEEDBACK327099 FOREIGN KEY (ID_CUSTOMER) REFERENCES CUSTOMER (ID_CUSTOMER);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING184382 FOREIGN KEY (ID_PAYMENT) REFERENCES PAYMENT (ID_PAYMENT);
ALTER TABLE Utility_POD ADD CONSTRAINT FKUtility_PO594193 FOREIGN KEY (ID_UTILITY) REFERENCES Utility (ID_UTILITY);
ALTER TABLE Utility_POD ADD CONSTRAINT FKUtility_PO654933 FOREIGN KEY (ID_POD) REFERENCES POD (ID_POD);
ALTER TABLE SLOT_BOOKING ADD CONSTRAINT FKSLOT_BOOKI249479 FOREIGN KEY (ID_SLOT) REFERENCES SLOT (ID_SLOT);
ALTER TABLE SLOT_BOOKING ADD CONSTRAINT FKSLOT_BOOKI140388 FOREIGN KEY (ID_BOOKING) REFERENCES BOOKING (ID_BOOKING);



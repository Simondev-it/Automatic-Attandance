CREATE TABLE BOOKING (
  ID             int IDENTITY NOT NULL, 
  ID_BOOKING     int NULL, 
  NAME_BOOKING   int NULL, 
  AMOUNT_BOOKING int NULL, 
  DETAIL_BOOKING int NULL, 
  STATUS_BOOKING int NULL, 
  PODID          int NOT NULL, 
  CUSTOMERID     int NOT NULL, 
  PAYMENTID      int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE [BOOKING PRODUCT] (
  ID                    int IDENTITY NOT NULL, 
  ID_BOOKINGPRODUCT     int NULL, 
  NAME_BOOKINGPRODUCT   int NULL, 
  AMOUNT_BOOKINGPRODUCT int NULL, 
  DETAIL_BOOKINGPRODUCT int NULL, 
  PRODUCTID             int NOT NULL, 
  BOOKINGID             int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE CUSTOMER (
  ID                int IDENTITY NOT NULL, 
  ID_CUSTOMER       int NULL, 
  NAME_CUSTOMER     int NULL, 
  EMAIL_CUSTOMER    int NULL, 
  PHONENB_CUSTOMER  int NULL, 
  BIRTHDAY_CUSTOMER int NULL, 
  TYPE_CUSTOMER     int NULL, 
  POINT_CUSTOMER    int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE FEEDBACK (
  ID              int IDENTITY NOT NULL, 
  ID_FEEDBACK     int NULL, 
  CMT_FEEDBACK    int NULL, 
  RATING_FEEDBACK int NULL, 
  DATE_FEEDBACK   int NULL, 
  BOOKINGID       int NOT NULL, 
  CUSTOMERID      int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE PAYMENT (
  ID             int IDENTITY NOT NULL, 
  ID_PAYMENT     int NULL, 
  METHOD_PAYMENT int NULL, 
  NAME_PAYMENT   int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE POD (
  ID                  int IDENTITY NOT NULL, 
  ID_POD              int NULL, 
  NAME_POD            int NULL, 
  STATUS_POD          int NULL, 
  PRICE_POD           int NULL, 
  RATING_POD          int NULL, 
  [POD TypeID]        int NOT NULL, 
  STOREID             int NOT NULL, 
  [POD PRICE FRAMEID] int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE [POD PRICE FRAME] (
  ID                int IDENTITY NOT NULL, 
  ID_PRICEFRAME     int NULL, 
  NAME_PRICEFRAME   int NULL, 
  DETAIL_PRICEFRAME int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE [POD Type] (
  ID            int IDENTITY NOT NULL, 
  ID_POD_TYPE   int NULL, 
  NAME_POD_TYPE int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE PRODUCT (
  ID             int IDENTITY NOT NULL, 
  ID_PRODUCT     int NULL, 
  NAME_PRODUCT   int NULL, 
  STATUS_PRODUCT int NULL, 
  RATING_PRODUCT int NULL, 
  PRICE_PRODUCT  int NULL, 
  STOREID        int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE SLOT (
  ID        int IDENTITY NOT NULL, 
  ID_SLOT   int NULL, 
  NAME_SLOT int NULL, 
  PODID     int NOT NULL, 
  PRIMARY KEY (ID));
CREATE TABLE SLOT_BOOKING (
  SLOTID    int NOT NULL, 
  BOOKINGID int NOT NULL, 
  PRIMARY KEY (SLOTID, 
  BOOKINGID));
CREATE TABLE STORE (
  ID         int IDENTITY NOT NULL, 
  ID_STORE   int NULL, 
  NAME_STORE int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE Utility (
  ID             int IDENTITY NOT NULL, 
  ID_UTILITY     int NULL, 
  NAME_UTILITY   int NULL, 
  DETAIL_UTILITY int NULL, 
  PRIMARY KEY (ID));
CREATE TABLE Utility_POD (
  UtilityID int NOT NULL, 
  PODID     int NOT NULL, 
  PRIMARY KEY (UtilityID, 
  PODID));
ALTER TABLE POD ADD CONSTRAINT FKPOD462531 FOREIGN KEY ([POD TypeID]) REFERENCES [POD Type] (ID);
ALTER TABLE POD ADD CONSTRAINT FKPOD973601 FOREIGN KEY (STOREID) REFERENCES STORE (ID);
ALTER TABLE PRODUCT ADD CONSTRAINT FKPRODUCT543967 FOREIGN KEY (STOREID) REFERENCES STORE (ID);
ALTER TABLE [BOOKING PRODUCT] ADD CONSTRAINT [FKBOOKING PR293503] FOREIGN KEY (PRODUCTID) REFERENCES PRODUCT (ID);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING54292 FOREIGN KEY (PODID) REFERENCES POD (ID);
ALTER TABLE [BOOKING PRODUCT] ADD CONSTRAINT [FKBOOKING PR184912] FOREIGN KEY (BOOKINGID) REFERENCES BOOKING (ID);
ALTER TABLE POD ADD CONSTRAINT FKPOD577906 FOREIGN KEY ([POD PRICE FRAMEID]) REFERENCES [POD PRICE FRAME] (ID);
ALTER TABLE SLOT ADD CONSTRAINT FKSLOT932876 FOREIGN KEY (PODID) REFERENCES POD (ID);
ALTER TABLE FEEDBACK ADD CONSTRAINT FKFEEDBACK295576 FOREIGN KEY (BOOKINGID) REFERENCES BOOKING (ID);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING383006 FOREIGN KEY (CUSTOMERID) REFERENCES CUSTOMER (ID);
ALTER TABLE FEEDBACK ADD CONSTRAINT FKFEEDBACK839338 FOREIGN KEY (CUSTOMERID) REFERENCES CUSTOMER (ID);
ALTER TABLE BOOKING ADD CONSTRAINT FKBOOKING919221 FOREIGN KEY (PAYMENTID) REFERENCES PAYMENT (ID);
ALTER TABLE Utility_POD ADD CONSTRAINT FKUtility_PO806749 FOREIGN KEY (UtilityID) REFERENCES Utility (ID);
